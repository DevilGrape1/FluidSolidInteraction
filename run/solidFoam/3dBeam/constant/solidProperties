FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      solidProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solidSolver unsTotalLagrangianSolid;

unsTotalLagrangianSolidCoeffs
{
    nCorrectors 10;
    convergenceTolerance 1e-6;
    nonLinear totalLagrangian;
    debug no;
    moveMesh no;

    componentReference ();
}

